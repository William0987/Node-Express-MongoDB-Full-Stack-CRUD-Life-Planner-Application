<%- include('../partials/header') %>
<h2 class="pageTitle" id="goalTitle">All Yearly Goals You Have Set</h2>
<table class="calendar">
  <thead>
    <tr>
      <th>Year</th>
      <th>Goal</th>
      <th>Completed (Yes/No)</th>
      <th>Details</th>
    </tr>
  </thead>
  <tbody>
    <% years.forEach(function(y) { %>
      <tr>
        <td><%= y.year %></td>
        <td><%= y.goal %></td>
        <td><%= y.completedCheck %></td>
        <td><a href="/years/<%= y._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>

  <a href="years/new"><button class="indexButton" id="addGoalBtn">Add New Year Goal</button></a>
  
<%- include('../partials/footer') %>